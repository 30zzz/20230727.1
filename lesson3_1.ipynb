   "source": [
    "#請使用者輸入梯形上下底及高、程式計算面積((上底+下底)/*高)\n",
    "top = float(input(\"請輸入梯形上底(公分): \"))\n",
    "bot = float(input(\"請輸入梯形下底(公分): \"))\n",
    "hight = float(input(\"請輸入梯形高(公分): \"))\n",
    "\n",
    "sum = (bot + top) * hight / 2\n",
    "\n",
    "print(\"梯形面積:\", round(sum, ndigits = 2), \"平方公分\")"
   ]
  }
